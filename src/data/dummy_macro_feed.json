{
  "macros": [
    {
      "id": "macro-1",
      "title": "Play/Pause",
      "keybind": "Ctrl + p",
      "isApproved": true,
      "applications": ["Spotify", "Google Chrome", "Discord"]
    },
    {
      "id": "macro-2",
      "title": "Volume Up",
      "keybind": "Ctrl + ↑",
      "isApproved": true,
      "applications": ["Spotify", "Google Chrome", "Discord", "Steam"]
    },
    {
      "id": "macro-3",
